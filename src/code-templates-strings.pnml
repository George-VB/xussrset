// Описание вагона
#define hint_wagon(name, cap, vol, lspeed, straging, strwagtype, from, to, factory)            \
switch (FEAT_TRAINS, SELF, name##_additional_text_shown,                                       \
[ STORE_TEMP(cap | (vol << 16), 0x100),                                                        \
  STORE_TEMP(lspeed | (string(straging) << 16), 0x101),                                        \
  STORE_TEMP(string(strwagtype) | (string(STR_PURCHASE_HINT_FROMTO, from, to) << 16), 0x102),  \
  STORE_TEMP(factory, 0x103) ])                                                                \
{ return string(STR_PURCHASE_HINT_WAGON); }                                                    \
switch (FEAT_TRAINS, SELF, name##_additional_text_hidden,                                      \
[ STORE_TEMP(cap | (vol << 16), 0x100),                                                        \
  STORE_TEMP(lspeed | (string(straging) << 16), 0x101),                                        \
  STORE_TEMP(string(strwagtype) | (string(STR_PURCHASE_HINT_FROM, from) << 16), 0x102),        \
  STORE_TEMP(factory, 0x103) ])                                                                \
{ return string(STR_PURCHASE_HINT_WAGON); }                                                    \
switch (FEAT_TRAINS, SELF, name##_additional_text, current_year <= to)                         \
{ 1: name##_additional_text_hidden;                                                            \
     name##_additional_text_shown; }                                                           \

