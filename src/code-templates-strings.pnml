// шаблон выбора по году
#define hint_change_after(name, year)                                     \
switch (FEAT_TRAINS, SELF, name##_additional_text, current_year >= year)  \
{ 0: name##_b##year##_additional_text;                                    \
     name##_a##year##_additional_text; }                                  \

