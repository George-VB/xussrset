// Описание МВПС c 2 производителями, 3 вариантами в СМЕ и примечанием
#define hint_MU_with_sme3_2fact_comment(name, lspeed, straging, strmutype, strengsmetype, strengsmevar1, strengsmevar2, strengsmevar3, from, to, factory1, factory2, comment)    \
switch (FEAT_TRAINS, SELF, name##_additional_text_shown,                                                                                                                         \
[ STORE_TEMP(lspeed | (string(straging) << 16), 0x100),                                                                                                                          \
  STORE_TEMP(string(strmutype) | (string(strengsmetype) << 16), 0x101),                                                                                                          \
  STORE_TEMP(string(STR_PURCHASE_HINT_LIST3, string(strengsmevar1), string(strengsmevar2), string(strengsmevar3)) | (string(STR_PURCHASE_HINT_FROMTO, from, to) << 16), 0x102),  \
  STORE_TEMP(string(STR_PURCHASE_HINT_LIST2) | (factory1 << 16), 0x103),                                                                                                         \
  STORE_TEMP(factory2 | (comment << 16), 0x104) ])                                                                                                                               \
{ return string(STR_PURCHASE_HINT_ENGINE_MU_COMMENT); }                                                                                                                          \
switch (FEAT_TRAINS, SELF, name##_additional_text_hidden,                                                                                                                        \
[ STORE_TEMP(lspeed | (string(straging) << 16), 0x100),                                                                                                                          \
  STORE_TEMP(string(strmutype) | (string(strengsmetype) << 16), 0x101),                                                                                                          \
  STORE_TEMP(string(STR_PURCHASE_HINT_LIST3, string(strengsmevar1), string(strengsmevar2), string(strengsmevar3)) | (string(STR_PURCHASE_HINT_LIST2) << 16), 0x102),             \
  STORE_TEMP(factory1 | (factory2 << 16), 0x103),                                                                                                                                \
  STORE_TEMP(comment, 0x104) ])                                                                                                                                                  \
{ return string(STR_PURCHASE_HINT_ENGINE_MU_COMMENT_NODATES); }                                                                                                                  \
switch (FEAT_TRAINS, SELF, name##_additional_text, current_year <= to)                                                                                                           \
{ 1: name##_additional_text_hidden;                                                                                                                                              \
     name##_additional_text_shown; }                                                                                                                                             \

